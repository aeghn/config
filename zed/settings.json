{
  "tabs": {
    "close_position": "left"
  },
  "auto_update": false,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "base_keymap": "Emacs",
  "ui_font_family": "Roboto",
  "buffer_font_family": "Courier Prime",
  "icon_theme": {
    "mode": "system",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "vim_mode": false,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "light",
    "light": "Fleet Light",
    "dark": "One Dark"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "title_bar": {
    "show_onboarding_banner": false,
    "show_user_picture": false,
    "show_sign_in": false
  },
  "agent": { "enabled": false, "button": false },
  "chat_panel": { "button": "never" },
  "collaboration_panel": { "button": false },
  "lsp": {
    "rust-analyzer": {
      "binary": {
        "path": "/usr/lib/rustup/bin/rust-analyzer"
      }
    }
  }
}
